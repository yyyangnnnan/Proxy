#!name=Supercell 请求修改模块
#!desc=只启用 MITM 并在 session.init 上执行脚本

[MITM]
hostname = %APPEND% id.supercell.com

[Script]
# 精确匹配 session.init，确保脚本在该请求上触发
supercell_geofaker = type=http-request,pattern=^https?://id\.supercell\.com/api/social/v3/session\.init,script-path=https://raw.githubusercontent.com/yyyangnnnan/Proxy/refs/heads/main/Supercell/supercell_geofaker.js,requires-body=true,timeout=10,debug=true

